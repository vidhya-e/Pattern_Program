OUTPUT: program 19:  
1
22
333
4444
55555

class Pattern_19 {
    public static void main(String[] args) {
        System.out.println("program 19:\n");
        int n=5,i,j;
        
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.print(i);
            }
            System.out.println();
        }
    }
}
