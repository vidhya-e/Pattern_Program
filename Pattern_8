OUTPUT: program 8
10000
10000
10000
10000
11111

class Pattern_8 {
    public static void main(String[] args) {
        System.out.println("program 8");
        int n=5,i,j;
        for(i=1;i<=5;i++)
        {
            for(j=1;j<=5;j++)
            {
                if(j==1 || i==n)
                System.out.print("1");
                else
                System.out.print("0");
            }
            System.out.println();
        }
    }
}
